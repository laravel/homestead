{
    "ip": "10.1.1.33",
    "memory": 4096,
    "cpus": 4,
    "provider": "virtualbox",
    "elasticsearch": "5.6.3",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "features": [
        {
            "elasticsearch": "5.6.3"
        },
        {
            "ohmyzsh": true
        }
    ],
    "folders": [
        {
            "map": "~/Code",
            "to": "/home/vagrant/Code"
        }
    ],
    "sites": [
        {
            "map": "admin.musicbed.test",
            "to": "/home/vagrant/Code/musicbed/musicbed/public",
            "php": "7.1"
        },
        {
            "map": "api3.musicbed.test",
            "to": "/home/vagrant/Code/musicbed/musicbed-api/public"
        },
        {
            "map": "sabre.musicbed.test",
            "to": "/home/vagrant/Code/musicbed/sabre/public",
            "php": "7.0"
        }
    ]
}
