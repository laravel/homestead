{
    "name": "musicbed",
    "ip": "10.1.1.33",
    "memory": 4096,
    "cpus": 4,
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": ["~/.ssh/id_rsa"],
    "features": [
        {
            "elasticsearch": {
                "version": "5"
            },
            "ohmyzsh": true
        }
    ],
    "folders": [
        {
            "map": "~/Code/musicbed/musicbed",
            "to": "/home/vagrant/musicbed",
            "type": "nfs"
        },
        {
            "map": "~/Code/musicbed/musicbed-api",
            "to": "/home/vagrant/musicbed-api",
            "type": "nfs"
        },
        {
            "map": "~/Code/musicbed/sabre",
            "to": "/home/vagrant/sabre",
            "type": "nfs"
        }
    ],
    "sites": [
        {
            "map": "admin.musicbed.test",
            "to": "/home/vagrant/musicbed/public",
            "php": "7.1"
        },
        {
            "map": "api3.musicbed.test",
            "to": "/home/vagrant/musicbed-api/public",
            "php": "7.1"
        },
        {
            "map": "sabre.musicbed.test",
            "to": "/home/vagrant/sabre/public",
            "php": "7.0"
        }
    ],
    "ports": [
        {
            "send": "8001",
            "to": "80"
        },
        {
            "send": "33061",
            "to": "3306"
        }
    ]
}
