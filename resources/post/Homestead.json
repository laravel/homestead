{
    "name": "post",
    "hostname": "post",
    "ip": "10.1.1.33",
    "memory": 4096,
    "cpus": 4,
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "features": [
        {
            "ohmyzsh": true
        }
    ],
    "folders": [
        {
            "map": "~/Code/musicbed/post-api-admin",
            "to": "/home/vagrant/post-api-admin",
            "type": "nfs"
        }
    ],
    "sites": [
        {
            "map": "api-admin.post.test",
            "to": "/home/vagrant/post-api-admin/public"
        }
    ],
    "databases": [
        "post"
    ],
    "ports": [
        {
            "send": "8002",
            "to": "80"
        },
        {
            "send": "33062",
            "to": "3306"
        }
    ]
}
